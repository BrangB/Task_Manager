generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user_data {
  id         String      @id @default(auto()) @map("_id") @db.ObjectId
  user_id    String
  email      String
  password   String
  createdat  DateTime    @default(now())
  updatedat  DateTime    @updatedAt
}

